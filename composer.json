{
	"name": "staabm/phpstan-dba",
	"license": "MIT",
	"require": {
		"php": "^8.0",
		"ext-mysqli": "*",
		"phpstan/phpstan": "^1.2"
	},
	"require-dev": {
		"ext-pdo": "*",
		"phpunit/phpunit": "^9",
		"phpstan/phpstan-php-parser": "^1.0",
		"phpstan/phpstan-strict-rules": "^1.1",
		"phpstan/phpstan-phpunit": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"staabm\\PHPStanDba\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"staabm\\PHPStanDba\\tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
